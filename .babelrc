{
	"presets": [
		[
			"es2015",
			{
				"modules": false
			}
		],
		"react",
		"stage-0"
	],
	"plugins": [
		"transform-runtime",
		"transform-decorators-legacy"
	],
	"env": {
		"produciton": {
			"only": [
				"src"
			],
			"plugins": [
				"transform-react-remove-prop-types",
				"transform-react-constant-elements",
				"transform-react-inline-elements"
			]
		}
	}
}
