{
  "name": "react-kickstart",
  "version": "6.1.4",
  "description": "just another react + webpack boilerplate.",
  "private": true,
  "author": "Alessandro Arnodo <alessandro@arnodo.net> (https://alessandro.arnodo.net)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vesparny/react-kickstart/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vesparny/react-kickstart.git"
  },
  "homepage": "https://github.com/vesparny/react-kickstart",
  "main": "index.js",
  "scripts": {
    "test": "npm run lint",
    "dev": "node devServer",
    "start": "cross-env NODE_ENV=production node prodServer",
    "bump": "standard-version",
    "prebuild": "npm run build:clean",
    "build:clean": "rimraf build",
    "build": "npm run lint && cross-env NODE_ENV=production webpack --config webpack.config.prod.js --color -p --progress",
    "gh-pages": "gh-pages --dist build",
    "release": "npm run test && npm run lint && npm run bump && npm run build && git push --follow-tags origin master && npm run gh-pages",
    "precommit": "lint-staged",
    "lint": "eslint --ext .js,.jsx .",
    "lint:fix": "npm run lint -- --fix",
    "lint:staged": "lint-staged"
  },
  "lint-staged": {
    "*.js": "eslint",
    "*.jsx": "eslint"
  },
  "dependencies": {
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "classnames": "^2.2.5",
    "eslint": "^4.7.2",
    "eslint-config-airbnb-base": "^12.0.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "normalize.css": "^7.0.0",
    "postcss": "^6.0.11",
    "postcss-cssnext": "^3.0.2",
    "postcss-flexbugs-fixes": "^3.2.0",
    "postcss-focus": "^2.0.0",
    "postcss-loader": "^2.0.6",
    "postcss-utilities": "^0.7.0",
    "precss": "^2.0.0",
    "prop-types": "^15.5.9",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-router-dom": "^4.1.1",
    "reflexbox": "^3.0.0-0",
    "riek": "^1.1.0",
    "sanitize.css": "^5.0.0",
    "webpack-combine-loaders": "^2.0.3"
  },
  "optionalDependencies": {
    "fsevents": "*"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.1",
    "enzyme": "^3.0.0",
    "enzyme-to-json": "^3.0.1",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.1",
    "gh-pages": "^1.0.0",
    "html-webpack-plugin": "^2.28.0",
    "husky": "^0.14.3",
    "jest-cli": "^21.2.1",
    "json-loader": "^0.5.4",
    "lint-staged": "^4.2.3",
    "lodash": "^4.13.1",
    "prettier": "^1.3.1",
    "react-addons-test-utils": "^15.5.1",
    "redbox-react": "^1.3.6",
    "rimraf": "^2.6.1",
    "standard": "^10.0.2",
    "standard-version": "^4.0.0",
    "style-loader": "^0.18.2",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.4.5"
  }
}
